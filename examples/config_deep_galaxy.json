{
  "experiment": "DeepGalaxy",
  "model": {
    "name": "UNetv2",
    "kwargs": {
      "dims": 2,
      "in_channels": 1,
      "out_channels": 1,
      "model_channels": 32,
      "num_res_blocks": 2,
      "data_shape": [
        128,
        128
      ],
      "attention_resolutions": [
        16,
        8
      ],
      "use_scale_shift_norm": true,
      "num_heads": 4,
      "num_classes": 25,
      "activation": "SiLU",
      "use_new_attention_order": false,
      "cond_fn": "MultiEmbeddings"
    }
  },
  "dataset": {
    "name": "DeepGalaxyDataset",
    "kwargs": {
      "path": "../../datasets/DeepGalaxy/output_bw_512.h5",
      "use_emb_as_labels": false,
      "dset_name_pattern": "s_*",
      "camera_pos": [0],
      "t_lim": [
        400,
        520
      ]
    }
  },
  "optimizer": {
    "name": "AdamW",
    "kwargs": {
      "lr": 0.0001
    }
  },
  "lr_scheduler": {
    "name": "CosineAnnealingLR",
    "kwargs": {
      "T_max": 10,
      "eta_min": 0.00001
    }
  },
  "noise_schedule": {
    "name": "CosineBetaSchedule",
    "kwargs": {
      "num_steps": 100,
      "offset": 0.008
    }
  },
  "training": {
    "device": "xpu",
    "np": 1,
    "save_checkpoint_every_n_epochs": 100,
    "sample_every_n_epochs": 30,
    "loss_fn": "MSELoss",
    "ema_decay": 0.0,
    "batch_size": 64,
    "min_epochs": 5,
    "max_epochs": 500,
    "seed": 777,
    "benchmark_mode": true
  },
  "inference": {
    "device": "xpu",
    "checkpoint": "model.pth",
    "parameter_space": {
      "s": [
        0.25, 0.5, 1
      ],
      "m": [
        1
      ],
      "t": [
        400,
        405,
        410,
        415,
        420,
        425,
        430,
        435,
        440,
        445,
        450,
        455,
        460,
        465,
        470,
        475,
        480,
        485,
        490,
        495,
        500
      ],
      "c": [
        0
      ]
    },
    "cache_file": "inference_output_deepgalaxy.h5",
    "plot_output_file": "inference_output_deepgalaxy.png",
    "seed": 777
  }
}
