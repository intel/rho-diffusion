{
  "experiment": "ShpericalHarmonics",
  "model": {
    "name": "UNetv2",
    "kwargs": {
      "dims": 3,
      "in_channels": 1,
      "out_channels": 1,
      "model_channels": 64,
      "num_res_blocks": 2,
      "data_shape": [
        32,
        32,
        32
      ],
      "attention_resolutions": [
        16,
        8
      ],
      "use_scale_shift_norm": true,
      "num_heads": 4,
      "num_classes": 20,
      "activation": "SiLU",
      "use_new_attention_order": false
    }
  },
  "dataset": {
    "name": "SphericalHarmonicDataset",
    "kwargs": {
      "max_l": 5
    }
  },
  "optimizer": {
    "name": "AdamW",
    "kwargs": {
      "lr": 0.0001
    }
  },
  "lr_scheduler": {
    "name": "CosineAnnealingLR",
    "kwargs": {
      "T_max": 10,
      "eta_min": 0.00001
    }
  },
  "noise_schedule": {
    "name": "LinearSchedule",
    "kwargs": {
      "num_steps": 1000,
      "beta_1": 0.001,
      "beta_T": 0.02
    }
  },
  "training": {
    "device": "xpu",
    "np": 1,
    "save_checkpoint_every_n_epochs": 10,
    "sample_every_n_epochs": 0,
    "loss_fn": "MSELoss",
    "ema_decay": 0.0,
    "batch_size": 32,
    "min_epochs": 50,
    "max_epochs": 1000,
    "seed": 777,
    "benchmark_mode": true
  },
  "inference": {
    "device": "xpu",
    "checkpoint": "model.pth",
    "parameter_space": {
      "l": [
        -5,
        -4,
        -3,
        -2,
        -1,
        1,
        2,
        3,
        4,
        5
      ],
      "m": [
        -5,
        -4,
        -3,
        -2,
        -1,
        1,
        2,
        3,
        4,
        5
      ]
    },
    "cache_file": "inference_output_spherical_harmonics.h5",
    "plot_output_file": null,
    "seed": 777
  }
}
